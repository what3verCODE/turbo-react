    "eslint": "~8.57.0",
    "prettier": "~3.2.5",
    "rimraf": "~5.0.7",
    "syncpack": "~12.3.3",
    "turbo": "~2.0.6",
    "@typescript-eslint/eslint-plugin": "~7.15.0",
    "@typescript-eslint/parser": "~7.15.0",
    "eslint-config-prettier": "~9.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
